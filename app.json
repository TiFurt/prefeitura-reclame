{
  "expo": {
    "name": "Prefeitura Reclame",
    "slug": "prefeitura-reclame",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/univassouras.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Permiter $(PRODUCT_NAME) tenha acesso a sua c√¢mera",
          "microphonePermission": "Permitir $(PRODUCT_NAME) tenha acesso ao seu microfone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ]
  }
}